{
  "name": "rolling-average",
  "version": "1.0.3",
  "author": "Tyson Jones",
  "description": "A simple rolling average calculator",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/ChapterSevenSeeds/rolling-average"
  },
  "license": "ISC",
  "main": "dist-commonjs/index.js",
  "module": "dist-esm/index.js",
  "types": "dist-esm/index.d.ts",
  "keywords": [
    "rolling",
    "average",
    "calculator",
    "moving"
  ],
  "homepage": "https://github.com/ChapterSevenSeeds/rolling-average",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.1.2",
    "typescript": "^5.3.2"
  },
  "scripts": {
    "test": "jest",
    "build-commonjs": "tsc --module commonjs --outDir dist-commonjs",
    "build-esm": "tsc --module esnext --outDir dist-esm",
    "build": "pnpm build-commonjs && pnpm build-esm",
    "clean": "rimraf dist-commonjs && rimraf dist-esm",
    "publish-package": "pnpm rebuild && pnpm test && pnpm publish",
    "rebuild": "pnpm clean && pnpm build"
  }
}
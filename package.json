{
  "name": "rolling-average",
  "version": "1.0.7",
  "author": "Tyson Jones",
  "description": "A simple rolling average calculator",
  "repository": {
    "type": "git",
    "url": "https://github.com/ChapterSevenSeeds/rolling-average"
  },
  "license": "ISC",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "keywords": [
    "rolling",
    "average",
    "calculator",
    "moving"
  ],
  "homepage": "https://github.com/ChapterSevenSeeds/rolling-average",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.1.2",
    "typescript": "^5.3.2"
  },
  "scripts": {
    "test": "jest",
    "build": "tsc --module esnext --outDir dist/esm && tsc --module commonjs --outDir dist/cjs && node package-fixup.js",
    "clean": "rimraf dist",
    "publish-package": "pnpm rebuild && pnpm test && pnpm publish",
    "rebuild": "pnpm clean && pnpm build"
  }
}
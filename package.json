    ".": {
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js"
    }
  },
  "keywords": [
    "rolling",
    "average",
    "calculator",
    "moving"
  ],
  "homepage": "https://github.com/ChapterSevenSeeds/rolling-average",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.1.2",
    "typescript": "^5.3.2",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4"
  },
  "scripts": {
    "test": "jest",
    "build": "webpack --mode=production --progress && node package-fixup.js",
    "clean": "rimraf dist",
    "publish-package": "pnpm test && pnpm run rebuild && pnpm publish",
    "rebuild": "pnpm clean && pnpm build"

{
  "name": "rolling-average",
  "version": "1.1.2",
  "author": "Tyson Jones",
  "description": "A simple rolling average calculator",
  "repository": {
    "type": "git",
    "url": "https://github.com/ChapterSevenSeeds/rolling-average"
  },
  "license": "ISC",
  "types": "./dist/esm/index.d.ts",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
